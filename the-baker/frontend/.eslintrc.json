{
  "extends": [ "airbnb" ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "arrow-parens": ["warn", "as-needed"],
    "camelcase": ["warn", {"allow": ["^UNSAFE_"], "properties": "never"}],
    "comma-dangle": ["warn", "never"],
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-quotes": ["warn", "prefer-single"],
    "jsx-a11y/mouse-events-have-key-events": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/control-has-associated-label": 0,
    "jsx-a11y/anchor-has-content": 0,
    "lines-between-class-members": 0,
    "max-len": 0,
    "no-confusing-arrow": ["warn", {"allowParens": true}],
    "no-console": ["warn", {"allow": ["warn", "error"]}],
    "no-multi-spaces": ["warn", {"exceptions": {"VariableDeclarator": true}}],
    "no-nested-ternary": 0,
    "operator-linebreak": ["warn", "after", { "overrides": { "?": "ignore", ":": "ignore" } }],
    "object-curly-newline": ["warn", {"multiline": true}],
    "object-curly-spacing": ["warn", "always"],
    "one-var": 0,
    "one-var-declaration-per-line": ["warn", "initializations"],
    "react/destructuring-assignment": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": ["warn", {"extensions": [".js", ".jsx"]}],
    "react/jsx-tag-spacing": ["warn", {"beforeSelfClosing": "never"}],
    "react/jsx-props-no-spreading": 0,
    "react/jsx-curly-newline": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-max-props-per-line": ["warn", {"maximum": 10}],
    "react/jsx-one-expression-per-line": 0,
    "react/no-array-index-key": 0,
    "react/no-danger": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/state-in-constructor": 0,
    "semi": ["warn", "never"]
  }
}
